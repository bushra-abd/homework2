#include<iostream>
using namespace std ;
int main()
{
 int n1,n2 ;
 int sum=0 ;
 do 
 { 
    cout<<"pls input first number(least) and final number(most) :";
    cin>>n1>>n2;
    }while ((n1>n2 ) || (n1<0 || n2<0));
    
  
    for (int i =n1; i<=n2;i++){
     
     sum+=i;
    }
    cout<<sum;
    
    return 0;
    
}
